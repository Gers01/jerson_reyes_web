@model List<proyectofinal.Models.prestamos>

@{
    ViewData["titulo"] = "pagina de inicio";
}


<h1 class="text-info">informe de prestamos</h1>

<div>
    <table class="table table-bordered table-hover table-striped">
        <thead>
            <tr>
                <th>
                    Nombre
                </th>
                <th>
                    Direccion
                </th>
                <th>
                    Telefono
                </th>
                <th>
                    Edad
                </th>
            </tr>
        </thead>
        <tbody>
            @{ 
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Nombre</td>
                        <td>@item.Direccion</td>
                        <td>@item.Edad</td>
                        <td>@item.Telefono</td>
                    </tr>
                }
            }
        </tbody>

    </table>
</div>

<div>
    <a class="btn btn-outline-primary">Nuevo</a>
</div>

<hr />

@DateTime.Now.ToShortDateString()
